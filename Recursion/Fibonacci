def fib_rec(n):  # fibonacci with recursion
    if n == 1 or n == 2:
        return 1
    else:
        return fib_rec(n-1)+fib_rec(n-2)


print(fib_rec(8))


def fib_iter(n):  # fibonacci with iteratively
    a, b = 0, 1
    for i in range(n):
        a, b = b, a+b
    return a


print(fib_iter(8))
